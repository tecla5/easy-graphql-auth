{
  "name": "gc-auth0-jquery-app",
  "version": "1.0.0",
  "description": "GraphCool Auth0 demo app based on jQuery",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ava",
    "build": "babel app.js -o app.js",
    "watch": "babel app.js --watch -o app.js"
  },
  "keywords": [
    "apollo",
    "graphcool",
    "auth0",
    "jquery"
  ],
  "author": "Kristian Mandrup",
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^1.2.2",
    "@tecla5/gc-auth0-apollo": "*"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 1,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}